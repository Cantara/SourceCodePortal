{
  "title": "Cantara Source Code Portal",
  "github": {
    "organization": "Cantara",
    "repos": [
      {
        "groupId": "SourceCodePortal-t",
        "repo": "SourceCodePortal",
        "branch": "master"
      },
      {
        "groupId": "Whydah-t",
        "repo": "Whydah-*",
        "branch": "master"
      },
      {
        "groupId": "ConfigService-t",
        "repo": "/\\b(?:ConfigService|JAU|Java-Auto-Update)\\b/gi",
        "branch": "master"
      },
      {
        "groupId": "Baseline-Projects-t",
        "repo": "[^\\s]*aseline",
        "branch": "master"
      },
      {
        "groupId": "Cantara-All-t",
        "repo": "[^\\s]*",
        "branch": "master"
      }
    ]
  }
}
