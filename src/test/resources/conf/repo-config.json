{
  "title": "Cantara Source Code Portal",
  "capraconsulting": {
    "provider": "github",
    "repos": [
      {
        "repo-id": "alexa",
        "repo-match": [
          "alexa-kurs-2018f"
        ],
        "branch": "master"
      }
    ]
  },
  "Cantara": {
    "provider": "github",
    "repos": [
      {
        "repo-id": "scp",
        "repo-match": [
          "SourceCodePortal"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": "Cantara-"
          },
          "snyk": {
            "prefix": "Cantara-"
          }
        }
      },
      {
        "repo-id": "Whydah",
        "repo-match": [
          "Whydah*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "ConfigService",
        "repo-match": [
          "ConfigService*",
          "JAU",
          "Java-Auto-Update"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "IOT",
        "repo-match": [
          "IoT*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "ValueReporter",
        "repo-match": [
          "Value*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "dropwizard",
        "repo-match": [
          "dropwizard*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "Flow",
        "repo-match": [
          "Flow*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "BaselineProjects",
        "repo-match": [
          "[^\\s]*aseline*",
          "BaseCommands",
          "BaseJDBCCommands"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "ACS",
        "repo-match": [
          "ACS",
          "Awesome"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "Cantara-All",
        "repo-match": [
          "[^\\s]*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      },
      {
        "repo-id": "ratpack",
        "repo-match": [
          "ratpack*"
        ],
        "branch": "master",
        "external": {
          "jenkins": {
            "prefix": ""
          },
          "snyk": {
            "prefix": ""
          }
        }
      }
    ]
  },
  "groups": [
    {
      "group-id": "SourceCodePortal",
      "display-name": "Source Code Portal",
      "description": "The SCP gathers all content from your GitHub repos and convey them in a easy grouped and readable form",
      "default-entry-repo": "github:Cantara/scp/SourceCodePortal",
      "repos-selector": [
        "github:Cantara/scp",
        "github:capraconsulting/alexa"
      ]
    },
    {
      "group-id": "Whydah",
      "display-name": "Whydah IAM and SSO Platform",
      "description": "Defcon 5 grade IAM and SSO security solution",
      "default-entry-repo": "github:Cantara/Whydah/Whydah-SecurityTokenService",
      "repos-selector": [
        "github:Cantara/Whydah"
      ]
    },
    {
      "group-id": "ConfigService",
      "display-name": "The ConfigService and JAU Toolchain",
      "description": "Provision configuration to agents",
      "default-entry-repo": "github:Cantara/ConfigService/ConfigService",
      "repos-selector": [
        "github:Cantara/ConfigService"
      ]
    },
    {
      "group-id": "IOT",
      "display-name": "Cantara IOT platform projects",
      "description": "Bulding blocks to create IOT platforms",
      "default-entry-repo": "github:Cantara/IOT/IoT-Service",
      "repos-selector": [
        "github:Cantara/IOT"
      ]
    },
    {
      "group-id": "ValueReporter",
      "display-name": "ValueReporter Toolchain",
      "description": "Toolchain to measure and detect valuable and dead parts of solutions",
      "default-entry-repo": "github:Cantara/ValueReporter/ValueReporter",
      "repos-selector": [
        "github:Cantara/ValueReporter"
      ]
    },
    {
      "group-id": "dropwizard",
      "display-name": "dropwizard examples",
      "description": "Getting started with dropwizard",
      "default-entry-repo": "github:Cantara/dropwizard/dropwizard-microservice-archetype",
      "repos-selector": [
        "github:Cantara/dropwizard"
      ]
    },
    {
      "group-id": "Flow",
      "display-name": "Cantara Flow",
      "description": "Graphics tracing and visualization of long-running processes",
      "default-entry-repo": "github:Cantara/Flow/FlowLogger",
      "repos-selector": [
        "github:Cantara/Flow"
      ]
    },
    {
      "group-id": "BaselineProjects",
      "display-name": "Cantara Baseline Codebases",
      "description": "Cantara Baseline projects for Java and Docker",
      "default-entry-repo": "github:Cantara/BaselineProjects/microservice-baseline",
      "repos-selector": [
        "github:Cantara/BaselineProjects"
      ]
    },
    {
      "group-id": "ACS",
      "display-name": "Awesome Competence System",
      "description": "The best competenece system",
      "default-entry-repo": "github:Cantara/ACS/Awesome-Competence-System",
      "repos-selector": [
        "github:Cantara/ACS"
      ]
    },
    {
      "group-id": "Cantara-All",
      "display-name": "Cantara GitHub, complete list of reposositories",
      "description": "All Cantara projects on GitHub",
      "default-entry-repo": "github:Cantara/Cantara-All/PathWatcher",
      "repos-selector": [
        "github:Cantara/Cantara-All"
      ]
    },
    {
      "group-id": "ratpack",
      "display-name": "ratpack examples",
      "description": "Getting started with ratpack",
      "default-entry-repo": "github:Cantara/ratpack/ratpack-websockets",
      "repos-selector": [
        "github:Cantara/ratpack"
      ]
    }
  ]
}
