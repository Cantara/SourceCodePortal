{
  "title": "Cantara Source Code Portal",
  "github": {
    "organization": "Cantara",
    "repos": [
      {
        "groupId": "SourceCodePortal",
        "display-name": "Source Code Portal",
        "description": "The SCP gathers all content from your GitHub repos and convey them in a easy grouped and readable form",
        "repo": "SourceCodePortal",
        "branch": "master"
      },
      {
        "groupId": "Whydah",
        "display-name": "Whydah IAM and SSO",
        "description": "Defcon 5 grade IAM and SSO security solution",
        "repo": "Whydah*",
        "branch": "master"
      },
      {
        "groupId": "ConfigService",
        "display-name": "ConfigurationAervice",
        "description": "Provision configuration to agents",
        "repo": "[^\\s]*/\\\b(?:ConfigService|JAU|Java-Auto-Update)\\\b*",
        "branch": "master"
      },
      {
        "groupId": "BaselineProjects",
        "display-name": "Cantara Baseline Codebases",
        "description": "Cantara Baseline projects for Java and Docker",
        "repo": "[^\\s]*aseline*",
        "branch": "master"
      },
      {
        "groupId": "Cantara-All",
        "display-name": "Cantara all",
        "description": "All Cantara projects on GitHub",
        "repo": "[^\\s]*",
        "branch": "master"
      }
    ]
  }
}
